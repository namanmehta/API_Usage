<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - API usage examples</title>
  <link rel="stylesheet" href="~/css/site.css" />
  @*<script type="text/javascript" src="~/js/Chart.min.js"></script>*@
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
  <script type="text/javascript" src="~/lib/chartjs-plugin-annotation-master/chartjs-plugin-annotation.min.js"></script>
  <script type="text/javascript" src="~/js/site.js"></script>
</head>
<body>
  <nav>
      <div class="nav-bar">
          <a asp-area="" asp-controller="Home" asp-action="Index">Home </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Symbols">Symbols </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Chart">Chart </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Refresh">Refresh </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="News">News</a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Crypto">Crypto</a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Sector">Sector Performance</a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Stats">Top Stock</a><text> | </text>
          @*<a asp-area="" asp-controller="Home" asp-action="Sector1">Sector Details</a><text> | </text>*@
          <a asp-area="" asp-controller="Home" asp-action="financials">Financial Details</a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="About">About Assignment</a><text> | </text>

      </div>
  </nav>
  <div class="container body-content">
      @RenderBody()
      <hr />
      @*<p>Please Povide your details here</p>
      <p>
          First name:     <input type="text" name="fname"><br>
          Last name:      <input type="text" name="lname"><br>
          <form asp-area="" asp-controller="Home" asp-action="About">
             <input type="submit" value="Submit My data" />
          </form>
      </p>*@

      <footer>
          <p>ISDS, Muma College of Business, University of South Florida</p>
      </footer>
  </div>
  @RenderSection("Scripts", required: false)
</body>
</html>